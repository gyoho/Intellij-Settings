<application>
  <component name="CodeInsightSettings">
    <option name="SHOW_PARAMETER_NAME_HINTS_ON_COMPLETION" value="true" />
    <EXCLUDED_PACKAGE NAME="akka.stream.ActorMaterializer" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseOneLineMethods" value="false" />
  </component>
  <component name="ScalaCodeFoldingSettings">
    <option name="collapseBlockComments" value="true" />
    <option name="foldingForAllBlocks" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bkey\b.*">
      <option name="FOREGROUND" value="7282ff" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern pattern="\bnote\b.*" />
    <pattern useCustomColors="true" pattern="\bdiscovery\b.*">
      <option name="FOREGROUND" value="c03b3f" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>